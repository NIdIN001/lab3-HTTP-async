<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.MenuController">
   <children>
      <Button fx:id="findButton" mnemonicParsing="false" onAction="#find" prefHeight="27.0" prefWidth="60.0" text="find" AnchorPane.leftAnchor="330.0" AnchorPane.topAnchor="10.0" />
      <TextField fx:id="textField" prefHeight="27.0" prefWidth="320.0" AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="10.0" />
      <ScrollPane layoutX="8.0" layoutY="37.0" prefHeight="352.0" prefWidth="382.0">
         <content>
            <VBox fx:id="vbox" alignment="TOP_CENTER" prefHeight="350.0" prefWidth="374.0" />
         </content></ScrollPane>
      <Pane prefHeight="200.0" prefWidth="300.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="400.0">
         <children>
            <Label fx:id="feelsLikeLabel">
               <padding>
                  <Insets top="20.0" />
               </padding>
            </Label>
            <Label fx:id="temperatureLabel" />
            <Label fx:id="humidityLabel">
               <padding>
                  <Insets top="40.0" />
               </padding>
            </Label>
            <Label fx:id="windSpeedLabel">
               <padding>
                  <Insets top="60.0" />
               </padding>
            </Label>
         </children>
      </Pane>
      <Pane prefHeight="170.0" prefWidth="300.0" AnchorPane.leftAnchor="400.0" AnchorPane.topAnchor="10.0">
         <children>
            <Label fx:id="placeLon">
               <padding>
                  <Insets left="120.0" top="80.0" />
               </padding>
            </Label>
            <Label fx:id="placeLat">
               <padding>
                  <Insets top="80.0" />
               </padding>
            </Label>
            <Hyperlink fx:id="placeLink">
               <padding>
                  <Insets top="40.0" />
               </padding>
            </Hyperlink>
            <Label fx:id="placeName" />
            <Label fx:id="placeRate">
               <padding>
                  <Insets top="20.0" />
               </padding>
            </Label>
         </children>
      </Pane>
   </children>
</AnchorPane>
